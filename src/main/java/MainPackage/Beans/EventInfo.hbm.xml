<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="MainPackage.Beans.EventInfo" table="EventInfo" schema="dbo" catalog="CampRandoApp">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="infoType">
            <column name="infoType" sql-type="varchar(64)" length="64"/>
        </property>
        <set name="eventAdditionalInfosById" inverse="true">
            <key>
                <column name="infoId"/>
            </key>
            <one-to-many not-found="ignore" class="MainPackage.Beans.EventAdditionalInfos"/>
        </set>
    </class>
</hibernate-mapping>