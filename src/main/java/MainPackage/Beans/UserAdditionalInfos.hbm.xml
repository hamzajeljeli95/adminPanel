<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="MainPackage.Beans.UserAdditionalInfos" table="UserAdditionalInfos" schema="dbo"
           catalog="CampRandoApp">
        <composite-id mapped="true" class="MainPackage.Beans.UserAdditionalInfosPK">
            <key-property name="id">
                <column name="id" sql-type="int"/>
            </key-property>
            <key-property name="userId">
                <column name="userId" sql-type="varchar(64)" length="64"/>
            </key-property>
            <key-property name="infoId">
                <column name="infoId" sql-type="int"/>
            </key-property>
        </composite-id>
        <property name="infoContent">
            <column name="InfoContent" sql-type="varchar(max)" length="2147483647"/>
        </property>
        <many-to-one name="appUserByUserId" class="MainPackage.Beans.AppUser">
            <column name="userId"/>
        </many-to-one>
        <many-to-one name="userInfoByInfoId" class="MainPackage.Beans.UserInfo">
            <column name="infoId"/>
        </many-to-one>
    </class>
</hibernate-mapping>